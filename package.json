  "bugs": {
    "url": "https://github.com/deepxmatter/DeepMatter-Tools-SimpleProjectTools/issues"
  },
  "homepage": "https://github.com/deepxmatter/DeepMatter-Tools-SimpleProjectTools#readme",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^17.0.5",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "chalk": "^4.1.0",
